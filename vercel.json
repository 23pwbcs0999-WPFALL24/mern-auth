{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/signup",
            "dest": "index.js"
        },
        {
            "src": "/api/signin",
            "dest": "index.js"
        },
        {
            "src": "/api/protected",
            "dest": "index.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "JWT_SECRET": "@jwt_secret"
    }
}